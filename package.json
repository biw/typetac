{
  "name": "typetac",
  "version": "0.0.0",
  "description": "tachyons in typescript",
  "main": "index.ts",
  "repository": "https://github.com/biw/typetac",
  "author": "Ben Williams <ben@biwills.com>",
  "license": "MIT",
  "scripts": {
    "_base_lint": "yarn eslint --config \".eslintrc.js\" ",
    "ci:lint": "yarn _base_lint \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "yarn _base_lint \"src/**/*.{ts,tsx,js,jsx,json,md}\" --fix",
    "precommit-lint": "yarn _base_lint --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/react-test-renderer": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json}": [
      "yarn precommit-lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "chai": "^4.2.0"
  }
}
